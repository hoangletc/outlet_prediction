      "username": "tommy"
    },
    {
      "previewLimit": 50,
      "server": "localhost",
      "port": 5432,
      "driver": "PostgreSQL",
      "name": "Outlet",
      "database": "outlet",
      "username": "tommy"
    }
  ]
